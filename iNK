-- INK Game GUI Script - Directo para Delta Executor

local Players = game:GetService("Players") local LocalPlayer = Players.LocalPlayer local Mouse = LocalPlayer:GetMouse() local UserInputService = game:GetService("UserInputService") local TweenService = game:GetService("TweenService") local CoreGui = game:GetService("CoreGui")

-- Crear GUI principal local ScreenGui = Instance.new("ScreenGui", CoreGui) ScreenGui.Name = "InkGUI"

local MainFrame = Instance.new("Frame") MainFrame.Size = UDim2.new(0, 450, 0, 280) MainFrame.Position = UDim2.new(0.3, 0, 0.2, 0) MainFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 25) MainFrame.BorderSizePixel = 0 MainFrame.Parent = ScreenGui MainFrame.ClipsDescendants = true MainFrame.Active = true MainFrame.Draggable = true

local UICorner = Instance.new("UICorner", MainFrame) UICorner.CornerRadius = UDim.new(0, 12)

local UIGrid = Instance.new("UIGridLayout", MainFrame) UIGrid.CellSize = UDim2.new(0, 140, 0, 40) UIGrid.CellPadding = UDim2.new(0, 10, 0, 10) UIGrid.FillDirection = Enum.FillDirection.Horizontal UIGrid.HorizontalAlignment = Enum.HorizontalAlignment.Center UIGrid.VerticalAlignment = Enum.VerticalAlignment.Top UIGrid.SortOrder = Enum.SortOrder.LayoutOrder

-- Scroll horizontal MainFrame.CanvasSize = UDim2.new(3, 0, 0, 280) MainFrame.ClipsDescendants = true MainFrame.AutomaticCanvasSize = Enum.AutomaticSize.None MainFrame.ScrollingEnabled = true MainFrame.ScrollBarThickness = 4 MainFrame.VerticalScrollBarInset = Enum.ScrollBarInset.Always

-- Sección: Luz Roja local luzRoja = Instance.new("TextButton") luzRoja.Text = "TP Final Luz Roja" luzRoja.BackgroundColor3 = Color3.fromRGB(255, 50, 50) luzRoja.TextColor3 = Color3.new(1,1,1) luzRoja.Parent = MainFrame luzRoja.MouseButton1Click:Connect(function() local finish = workspace:FindFirstChild("EndZone") or workspace:FindFirstChild("Finish") if finish then LocalPlayer.Character:PivotTo(finish.CFrame + Vector3.new(0, 3, 0)) end end)

-- Sección: Dalgona local dalgona = Instance.new("TextButton") dalgona.Text = "Auto Dalgona" dalgona.BackgroundColor3 = Color3.fromRGB(255, 170, 0) dalgona.TextColor3 = Color3.new(1,1,1) dalgona.Parent = MainFrame dalgona.MouseButton1Click:Connect(function() for _,v in pairs(getgc(true)) do if typeof(v) == "function" and getfenv(v).script.Name == "Dalgona" then pcall(function() v() end) end end end)

-- Sección: Escondidas ESP local espBtn = Instance.new("TextButton") espBtn.Text = "ESP Escondidas" espBtn.BackgroundColor3 = Color3.fromRGB(0, 150, 255) espBtn.TextColor3 = Color3.new(1,1,1) espBtn.Parent = MainFrame espBtn.MouseButton1Click:Connect(function() for _,v in pairs(Players:GetPlayers()) do if v ~= LocalPlayer and v.Character and v.Character:FindFirstChild("Head") then local box = Instance.new("BoxHandleAdornment", v.Character.Head) box.Size = Vector3.new(4,6,2) box.Adornee = v.Character box.AlwaysOnTop = true box.ZIndex = 10 box.Color3 = Color3.fromRGB(255,0,0) box.Transparency = 0.5 end end end)

-- Sección: Cuerda local cuerda = Instance.new("TextButton") cuerda.Text = "Remover Cuerda" cuerda.BackgroundColor3 = Color3.fromRGB(128, 64, 255) cuerda.TextColor3 = Color3.new(1,1,1) cuerda.Parent = MainFrame cuerda.MouseButton1Click:Connect(function() local rope = workspace:FindFirstChild("Rope") if rope then rope:Destroy() end end)

-- Botón ganar automático (Tug o similar) local autoBoton = Instance.new("TextButton") autoBoton.Text = "Auto Presionar Botón" autoBoton.BackgroundColor3 = Color3.fromRGB(50, 200, 80) autoBoton.TextColor3 = Color3.new(1,1,1) autoBoton.Parent = MainFrame autoBoton.MouseButton1Click:Connect(function() while true do task.wait(0.1) game:GetService("VirtualInputManager"):SendKeyEvent(true, "E", false, game) end end)

-- Safe zone local tpSafe = Instance.new("TextButton") tpSafe.Text = "TP Safe Zone" tpSafe.BackgroundColor3 = Color3.fromRGB(200, 200, 50) tpSafe.TextColor3 = Color3.new(1,1,1) tpSafe.Parent = MainFrame tpSafe.MouseButton1Click:Connect(function() LocalPlayer.Character:PivotTo(CFrame.new(0,200,0)) end)

-- Glass: Reveal local revealGlass = Instance.new("TextButton") revealGlass.Text = "Reveal Glass" revealGlass.BackgroundColor3 = Color3.fromRGB(50, 255, 255) revealGlass.TextColor3 = Color3.new(0,0,0) revealGlass.Parent = MainFrame revealGlass.MouseButton1Click:Connect(function() for _,v in pairs(workspace:GetDescendants()) do if v:IsA("Part") and v.Name == "Glass" then v.Transparency = 0.1 v.Color = Color3.fromRGB(0,255,0) end end end)

-- Salta la cuerda - TP local jumpEnd = Instance.new("TextButton") jumpEnd.Text = "TP Final Cuerda" jumpEnd.BackgroundColor3 = Color3.fromRGB(255, 100, 200) jumpEnd.TextColor3 = Color3.new(1,1,1) jumpEnd.Parent = MainFrame jumpEnd.MouseButton1Click:Connect(function() local pos = workspace:FindFirstChild("EndZone") or workspace:FindFirstChild("Goal") if pos then LocalPlayer.Character:PivotTo(pos.CFrame + Vector3.new(0,3,0)) end end)

-- Salta la cuerda - auto saltar local autoJump = Instance.new("TextButton") autoJump.Text = "Auto Completar Cuerda" autoJump.BackgroundColor3 = Color3.fromRGB(255, 140, 240) autoJump.TextColor3 = Color3.new(0,0,0) autoJump.Parent = MainFrame autoJump.MouseButton1Click:Connect(function() while true do task.wait(0.7) LocalPlayer.Character.Humanoid.Jump = true end end)

-- Salto infinito local infJump = Instance.new("TextButton") infJump.Text = "Salto Infinito" infJump.BackgroundColor3 = Color3.fromRGB(100, 255, 100) infJump.TextColor3 = Color3.new(0,0,0) infJump.Parent = MainFrame infJump.MouseButton1Click:Connect(function() UserInputService.JumpRequest:Connect(function() LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):ChangeState("Jumping") end) end)
